main:
  - title: "Detections"
    url: /detections/
  - title: "Analytic Stories"
    url: /stories/
  - title: "Playbooks"
    url: /playbooks/
  - title: "Blog"
    url: https://www.splunk.com/en_us/blog/author/secmrkt-research.html
  - title: "About"
    url: /about/
detections:
  - title: "Types"
    children:
    {%- for type in types %}
      - title: {{ type | capitalize }}
        url: /categories/{{ type }}
    {%- endfor %}
  - title: "Tactic"
    children:
    {%- for tactic in tactics %}
      - title: {{ tactic }}
        url: /tags/#{{ tactic | lower | replace(" ", "-") }}
    {%- endfor %}
  - title: "Datamodel"
    children:
    {%- for datamodel in datamodels %}
      - title: {{ datamodel }}
        url: /detections/{{ datamodel | lower | replace(" ", "_") }}/
    {%- endfor %}
  - title: "Product"
    children:
      - title: "Splunk Enterprise Security"
        url: /tags/#splunk-enterprise-security
      - title: "Splunk Behavioral Analytics"
        url: /tags/#splunk-behavioral-analytics
stories:
  - title: "Use Case"
    children:
    {%- for category in categories %}
      - title: {{ category }}
        url: /stories/{{ category | lower | replace(" ", "_") }}/
    {%- endfor %}
playbooks:
  - title: "Type"
    children:
    {%- for playbook_type in playbook_types %}
      - title: {{ playbook_type }}
        url: /playbooks/{{ playbook_type | lower | replace(" ", "_") }}/
    {%- endfor %}
  - title: "Use Case"
    children:
    {%- for playbook_use_case in playbook_use_cases %}
      - title: {{ playbook_use_case }}
        url: /playbooks/{{ playbook_use_case | lower | replace(" ", "_") }}/
    {%- endfor %}
  - title: "D3fend Technique Category"
    children:
    {%- for playbook_category in playbook_categories %}
      - title: {{ playbook_category }}
        url: /playbooks/{{ playbook_category | lower | replace(" ", "_") }}/
    {%- endfor %}
  - title: "Soar App"
    children:
    {%- for playbook_app in playbook_apps %}
      - title: {{ playbook_app }}
        url: /playbooks/{{ playbook_app | lower | replace(" ", "_") }}/
    {%- endfor %}